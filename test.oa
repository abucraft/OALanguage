class #Array{
	int[] @a;
	int @low;
	int @high;
}

class #Arrarysort extends #Array{
	void @Qsort();
}



void #Arrarysort::@Qsort() {
	if(@low >= @high) { 
		return;
	}
	int @first = @low;
	int @last = @high;
	int @key = @a[@first];
	while(@first < @last) {
		while( @first < @last && @a[@last] >= @key ) {
			@last = @last - 1;
		}
		@a[@first] = @a[@last];
		while( @first < @last && @a[@first] <= @key ) {
			@first = @first + 1;
		}
		@a[@last] = @a[@first];
	};
	@a[@first] = @key;
	@Qsort(@a, @low, @first-1);
	@Qsort(@a, @first+1, @high);
}

int @main() {
	#Arraysort @arraySort;
	@as.@a[0] = 57;
	@as.@a[1] = 68;
	@as.@a[2] = 59;
	@as.@a[3] = 52;
	@as.@a[4] = 72;
	@as.@a[5] = 28;
	@as.@a[6] = 96;
	@as.@a[7] = 33;
	@as.@a[8] = 24;
	@as.@Qsort();
	return 0;
}