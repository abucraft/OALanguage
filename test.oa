class #Array{
	int[] @a;
	int @low;
    int @high;
}

class #Arraysort extends #Array{
	void @Qsort();
}


void #Arraysort::@Qsort(int[] @a, int @l, int @h) {
	if(@l >= @h) { 
		return;
	}
	int @first = @l;
	int @last = @h;
	int @key = @a[@first];
	while(@first < @last) {
		while( @first < @last && @a[@last] >= @key ) {
			@last = @last - 1;
		}
		@a[@first] = @a[@last];
		while( @first < @last && @a[@first] <= @key ) {
			@first = @first + 1;
		}
		@a[@last] = @a[@first];
	};
	@a[@first] = @key;
	@Qsort(@a, @l, @first-1);
	@Qsort(@a, @first+1, @h);
}

int @main() {
	#Arraysort @as;
	@as.@a[0] = 57;
	@as.@a[1] = 68;
	@as.@a[2] = 59;
	@as.@a[3] = 52;
	@as.@a[4] = 72;
	@as.@a[5] = 28;
	@as.@a[6] = 96;
	@as.@a[7] = 33;
	@as.@a[8] = 24;
	@as.@Qsort(@as.@a, @as.@low, @as.@high);
	return 0;
}